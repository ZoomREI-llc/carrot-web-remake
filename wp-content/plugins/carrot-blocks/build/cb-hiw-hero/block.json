{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "carrot-blocks/cb-hiw-hero",
  "version": "0.1.0",
  "title": "Carrot Block Hero (cb-hiw)",
  "category": "widgets",
  "icon": "superhero-alt",
  "description": "A block to display the 'Carrot Block How-it-works-page Hero' section",
  "supports": {
    "html": false
  },
  "textdomain": "carrot-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js",
  "attributes": {
    "selectedMarket": {
      "type": "string",
      "default": "stl"
    },
    "phoneNumber": {
      "type": "string",
      "default": "(314) 887-8043 "
    },
    "formId": {
      "type": "string",
      "default": "1"
    }
  }
}