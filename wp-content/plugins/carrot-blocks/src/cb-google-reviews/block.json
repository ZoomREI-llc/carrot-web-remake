{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "carrot-blocks/cb-google-reviews",
	"version": "0.1.0",
	"title": "Google Reviews",
	"category": "widgets",
	"icon": "",
	"description": "A block to display the 'Google Reviews' section",
	"supports": {
		"html": false
	},
	"textdomain": "carrot-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js",
	"attributes": {
		"selectedDirection": {
			"type": "string",
			"default": "ltr"
		},
		"reviewText": {
			"type": "string",
			"default": ""
		},
		"reviewerName": {
			"type": "string",
			"default": ""
		}
	}
}
